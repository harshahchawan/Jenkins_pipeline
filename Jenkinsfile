

pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World1 this is jenkins'
                sleep(time: 10, unit: 'SECONDS')
            }
        }
        stage('Hello2') {
            steps {
                echo 'Hello World2 jenkins2'
                sleep(time: 10, unit: 'SECONDS')
             
            }
        }
        stage('Hello3') {
            steps {
                echo 'Hello World3'
            }
        }
    }
}
